{
  "log2console": {
    "debug": true,
    "info": true,
    "warn": true,
    "error": true
  },
  "timestamp": "YYYY-MM-DD HH:mm:ss",
  "levelShow": true,
  "lineInfo": true,
  "log2file":{
    "debug": true,
    "info": true,
    "warn": true,
    "error": true
  },
  "dirRoot": false,
  "dirs": ["logs", "$YYYY", "$MM", ":type"],
  "default_dir": "logs",
  "filename":"$MMDD",
  "levelfile": {
    "error": true
  },
  "filesize": "2 MB"
}